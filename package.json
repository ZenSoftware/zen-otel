{
  "name": "ts-express",
  "version": "0.0.0",
  "author": "",
  "description": "",
  "license": "MIT",
  "scripts": {
    "start": "tsc-watch --onSuccess \"node --inspect=5858 dist/main.js\"",
    "start:tracing": "node -r ./dist/tracing.js ./dist/main.js",
    "build": "rimraf dist && tsc",
    "docker:build": "npm run build && docker build -f ./Dockerfile -t ts-express .",
    "docker:run": "docker run -d --name ts-express -p 3001:3000 ts-express"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/exporter-zipkin": "^1.2.0",
    "@opentelemetry/instrumentation-express": "^0.28.0",
    "@opentelemetry/instrumentation-graphql": "^0.27.4",
    "@opentelemetry/instrumentation-http": "^0.28.0",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/tracing": "^0.24.0",
    "express": "^4.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
